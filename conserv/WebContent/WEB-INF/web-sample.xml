<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>NetDimensions Talent Suite Content Server</display-name>
  <filter>
    <filter-name>DelegatedAuthenticationFilter</filter-name>
    <filter-class>com.netdimensions.servlet.DelegatedAuthenticationFilter</filter-class>
    <init-param>
      <param-name>serviceUrl</param-name>
      <param-value>https://www.example.com/ekp/servlet/ekp/authenticate</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>DelegatedAuthenticationFilter</filter-name>
    <url-pattern>/nd/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>AuthenticationTokenVerifierServlet</servlet-name>
    <servlet-class>com.netdimensions.servlet.AuthenticationTokenVerifierServlet</servlet-class>
    <init-param>
      <param-name>digestAlgorithm</param-name>
      <param-value>MD5</param-value>
    </init-param>
    <init-param>
      <param-name>key</param-name>
      <param-value>changeme</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>AuthenticationTokenVerifierServlet</servlet-name>
    <url-pattern>/authenticationTokenVerifier</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>